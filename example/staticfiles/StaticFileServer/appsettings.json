{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "Knife": {
        "StaticFiles": {
            "/files": {
                "FolderPath": ".",
                "ServeUnknownFileTypes": true,
                "EnableDirectoryBrowsing": true
            },
            "dlls": {
                "FolderPath": "bin/Debug",
                "ServeUnknownFileTypes": true,
                "EnableDirectoryBrowsing": true
            }
        }
    }
}
