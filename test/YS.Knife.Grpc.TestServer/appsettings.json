{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        },
        "Console": {
            "IncludeScopes": true,
            "LogLevel": {
                "Default": "Warning",
                "YS.Knife.Hosting.Web.Middlewares.RequestLoggingMiddleware": "Information"
            }
        }
    },
    "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://localhost:5000",
        "Protocols": "Http2"
      },
      "https": {
        "Url": "https://localhost:5001",
        "Protocols": "Http1AndHttp2"
      }
    }
  }
}
